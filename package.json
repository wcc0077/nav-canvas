{
  "name": "free-nav",
  "displayName": "free-nav",
  "description": "tired of the tree explorer? try this free movable scalable file navigation",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.98.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "*"
  ],
  "main": "./dist/extension.js",
  "scripts": {
    "watch:tsc": "tsc -watch -p ./",
    "watch:esbuild": "node esbuild.js --watch",
    "watch": "concurrently \"pnpm run watch:tsc\" \"pnpm run watch:esbuild\""
  },
  "contributes": {
    "commands": [
      {
        "command": "nav-canvas.openNavigator",
        "title": "Open File Navigator"
      },
      {
        "command": "nav-canvas.openCanvas",
        "title": "Open Canvas",
        "icon": {
          "light": "resources/nav-icon.svg",
          "dark": "resources/nav-icon.svg"
        }
      }
    ],
    "viewsContainers": {
      "activitybar": [
        {
          "id": "nav-canvas",
          "title": "Free Nav",
          "icon": "resources/nav-icon.svg"
        }
      ]
    },
    "views": {
      "nav-canvas": [
        {
          "id": "nav-canvas-canvas-view",
          "name": "Canvas",
          "context": "navCanvasActive"
        }
      ]
    }
  },
  "devDependencies": {
    "@types/vscode": "^1.98.0",
    "@types/react": "^18.2.48",
    "@types/react-dom": "^18.2.18",
    "concurrently": "^9.1.2",
    "esbuild": "^0.19.11",
    "typescript": "^5.3.3"
  },
  "dependencies": {
    "@xyflow/react": "^12.0.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  }
}